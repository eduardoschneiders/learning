
public class Pessoa {
	String nome;
	int idade;
	
	public Pessoa(String nome, int idade) {
		nome = nome;
		idade = idade;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public int getIdade() {
		return idade;
	}

	public void setIdade(int idade) {
		this.idade = idade;
	}
	
	
	

}
